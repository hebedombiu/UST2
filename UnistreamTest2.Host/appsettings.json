{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "IncludeScopes": true,
        "SingleLine": true,
        "UseUtcTimestamp": true,
        "TimestampFormat": "HH:mm:ss.ffff"
      }
    }
  },
  "AllowedHosts": "*"
}